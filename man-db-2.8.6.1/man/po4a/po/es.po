msgid "Async"
msgstr "Asíncrono"

#. type: tbl table
#: ../../man/man8/mandb.man8:59
#, no-wrap
msgid "Filename"
msgstr "Nombre del fichero"

#. type: tbl table
#: ../../man/man8/mandb.man8:61
#, no-wrap
msgid "Berkeley db"
msgstr "DB Berkeley"

#. type: tbl table
#: ../../man/man8/mandb.man8:63
#, no-wrap
msgid "Binary tree"
msgstr "Árbol binario"

#. type: tbl table
#: ../../man/man8/mandb.man8:63 ../../man/man8/mandb.man8:66
